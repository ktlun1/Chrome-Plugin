{
	"name": "My Cookie Manager",
	"version": "0.0.0.2",
	"description": "My Cookie Manager",
	
	"background": {
		"scripts": ["cookie.js"]
	},
	"icons": { "16": "cookie.png","48": "cookie.png", "128": "cookie.png" },
	"browser_action": {
		"default_icon": {
			"19": "cookie.png",
			"38": "cookie.png"
		},
		"default_title": "My Cookie Manager"
	},
	"permissions": [ "tabs", "contextMenus","<all_urls>","storage","cookies","https://*/","http://*/"],
	"manifest_version": 2
}

