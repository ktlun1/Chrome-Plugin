{
    "name": "Youtube video downloader",
    "version": "1.0",
    "description": "Download youtube video.",
    "manifest_version": 2,
    "permissions": [
        "<all_urls>"
    ],
    "content_scripts": [
        {
            "matches": ["*://*.youtube.com/watch*"],
            "run_at": "document_end",
            "js": ["content.js"]
        }
    ]
}
